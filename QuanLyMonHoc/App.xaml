<Application x:Class="QuanLyMonHoc.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QuanLyMonHoc"
             xmlns:localModel="clr-namespace:QuanLyMonHoc.ViewModels"
             
             xmlns:localModels="clr-namespace:QuanLyMonHoc.Models"
             StartupUri="WindowChinh.xaml">
    <Application.Resources>
        <localModel:MonHocVM x:Key="rsMonHoc"/>
        <localModel:HocvienVM x:Key="rsHocvien"/>
        <x:Array x:Key="rsPhai" Type="localModels:Phais">
            <localModels:Phais key="True" value="Nam"/>
            <localModels:Phais key="False" value="Nữ"/>
        </x:Array>
    </Application.Resources>
</Application>
